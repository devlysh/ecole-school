{
  "name": "ecole-school",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "deploy": "cd ./infrastructure/aws && npm i && cdk synth --profile ecole && cdk deploy --profile ecole && cd ../..",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "npm run prisma:migrate",
    "migrate:local": "npx dotenv -e .env.local -- npm run migrate",
    "seed": "npm run prisma:seed",
    "seed:local": "npx dotenv -e .env.local -- npm run seed",
    "seed:admin": "tsx prisma/seed-admin.ts",
    "seed:admin:local": "npx dotenv -e .env.local -- npm run seed:admin",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "sync": "npm run stripe:sync",
    "sync:local": "npx dotenv -e .env.local -- npm run sync",
    "stripe:sync": "npm run stripe:sync:customers",
    "stripe:sync:plans": "tsx scripts/sync-stripe-plans.ts",
    "stripe:sync:customers": "tsx scripts/sync-stripe-customers.ts",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to http://localhost:3000/api/v1/stripe/webhook",
    "stripe:trigger:payment_succeeded": "stripe trigger invoice.payment_succeeded",
    "start:prod": "NODE_ENV=production dotenv -e .env.production -- next start",
    "build:prod": "NODE_ENV=production dotenv -e .env.production -- next build",
    "db:deploy:local": "cd infrastructure/local && docker-compose up -d && cd ../../",
    "db:destroy:local": "cd infrastructure/local && docker-compose down && npm run db:rm-volume:local && cd ../../",
    "db:rm-volume:local": "docker volume rm local_postgres_data",
    "db:init:local": "npm run migrate:local && npm run seed:local && npm run seed:admin:local && npm run sync:local"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/rrule": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@nextui-org/button": "^2.0.38",
    "@nextui-org/card": "^2.0.34",
    "@nextui-org/input": "^2.2.5",
    "@nextui-org/progress": "^2.0.34",
    "@nextui-org/react": "^2.4.8",
    "@nextui-org/spinner": "^2.0.34",
    "@nextui-org/system": "^2.2.6",
    "@nextui-org/theme": "^2.2.11",
    "@prisma/client": "^5.20.0",
    "@stripe/react-stripe-js": "^2.8.1",
    "@stripe/stripe-js": "^4.8.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "esm": "^3.2.25",
    "formik": "^2.4.6",
    "framer-motion": "^11.11.1",
    "install": "^0.13.0",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.22",
    "npm": "^10.8.3",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.2.2",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^11.0.3",
    "rrule": "^2.8.1",
    "stripe": "^17.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.13",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^2.7.0",
    "node-mocks-http": "^1.16.2",
    "prettier": "^3.4.2",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
